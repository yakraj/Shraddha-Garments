{
  "version": 2,
  "functions": {
    "server/src/index.ts": {
      "maxDuration": 10,
      "includeFiles": "server/prisma/**"
    }
  },
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/src/index.ts"
    },
    {
      "source": "/(.*)",
      "destination": "/client/index.html"
    }
  ]
}
