{
  "name": "shraddha-garments",
  "private": true,
  "type": "module",
  "workspaces": [
    "client",
    "server"
  ],
  "scripts": {
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "build:client": "cd client && npm run build",
    "build:server": "cd server && npx prisma@5.10.0 generate",
    "build": "npm run build:client && npm run build:server",
    "postinstall": "cd server && npx prisma@5.10.0 generate"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "@prisma/client": "5.10.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.9.0",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.1",
    "prisma": "5.10.0"
  },
  "devDependencies": {
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.19"
  }
}
